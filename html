<head> 
    <meta charset="UTF-8" />
    <title>Moedas na praia</title>
    <script src="//cdn.jsdelivr.net/npm/phaser@3.11.0/dist/phaser.js"></script>
    <style type="text/css">
        body {
            margin: 0;
        }
    </style>
</head>
<body>

<script type="text/javascript">

    var config = {
        type: Phaser.AUTO,
        width: 800,
        height: 625,
        scene: {
            preload: preload,
            create: create,
            update: update
        }
    };

    var game = new Phaser.Game(config);

    function preload ()
    {
        this.load.image('finalDay', 'Assets/finalDay.PNG');
        this.load.image('coin', 'Assets/coin.png', { frameWidth: 32, frameHeight: 48 });
        
    }

    function create ()
    {
        this.add.imgae(800,625, 'finalDay');
    }

    function update ()
    {
    }

</script>

</body>
</html>
